{
    "name": "snowsoft/oatinymce",
    "description": "Integrate tinymce into laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "editor", "tinymce"],
    "homepage": "https://github.com/snowsoft/tinymce",
    "license": "MIT",
    "authors": [
        {
            "name": "snowsoft",
            "email": "kadir@nlksoft.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "snowsoft/open-admin": ">=v0.01"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdmin\\Tinymce\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdmin\\Tinymce\\TinymceServiceProvider"
            ]

        }
    }
}
