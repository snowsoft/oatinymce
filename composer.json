{
    "name": "snowsoft/ckeditor",
    "description": "Integrate CKEDITOR into laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "editor", "CKEditor"],
    "homepage": "https://github.com/laravel-admin-ext/ckeditor",
    "license": "MIT",
    "authors": [
        {
            "name": "song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "snowsoft/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\CKEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\CKEditor\\CKEditorServiceProvider"
            ]

        }
    }
}
